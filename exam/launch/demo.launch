<launch>
    <param name="initial_cov_xx" type="double" value="1000" />
    <param name="initial_cov_yy" type="double" value="1000" />
    <param name="max_particles" type="int" value="5000000" />

    

    <!--run gazebo-->
    <include file="$(find map2gazebo)/launch/turtlebot3_world.launch">        
    </include>

    <!--run rviz-->
    <include file="$(find exam)/launch/turtlebot3_navigation.launch" >
        <arg name="map_file" value="$(find exam)/maps/map6.yaml" />
    </include>

    <!--run teleop key-->
    <!--include file="$(find turtlebot3_teleop)/launch/turtlebot3_teleop_key.launch" /-->

    <!-- run application node -->
    <!--node name="navigation_node" pkg="exam" type="main.py" output="screen" /-->

</launch>